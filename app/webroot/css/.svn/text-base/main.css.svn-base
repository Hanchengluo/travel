/*中间*/
#main{
	margin-top:1px;
	min-height:530px;
}
#main .left{
	width:310px;
	background:#edf5f7;
	float:left;
}
#main .left h2{
	display:block;
	height:42px;
	line-height:42px;
	color:#fff;
	text-indent:20px;
	background:url(../img/left_title_bg.jpg);
}
#main .left .btn-tool{
	padding:20px;
}
#main .left h3{
	display:block;
	height:27px;
	line-height:27px;
	text-indent:20px;
	border-top:1px solid #fff;
	border-bottom:1px solid #8dabb5;
	background:#e4eff3;
}
#main .left .form{
	padding:10px 20px;
	text-align:left;
}
#main .right{
	background:#fff;
	width: auto;
	padding-left:310px;
}
#main .right .head-title{
	height:42px;
	background:url(../img/right_title_bg.jpg) repeat-x;
	text-indent:20px;
	color:#5d818f;
	border-bottom:#8dabb6;
}
#main .right .block-content{
	padding:20px;
}
.column{
	padding:20px;
}
.table {
	border:1px solid #9bb3bd;
	border-collapse:none;
	margin-bottom:0;
	border-radius:0;
}
.table thead{
	background:url(../img/table_header_bg.jpg) repeat-x;
}
.table th, .table td{
	height:10px;
	line-height:10px;
	font-size:12px;
}
.table thead th{
	color:#333;
	border-left:1px solid #fff;
	border-left:1px solid #d1d1d1;
}
.pages{
	border:1px solid #9bb3bd;
	border-top:none;
	line-height:12px;
	height:10px;
	padding:8px;
}
.pages a{
	color:#666;
}
.pages a:hover{
	font-weight:bold;
}

#footer{
	height:35px;
	line-height:35px;
	text-indent:20px;
	background:url(../img/footer_bg.jpg);
	border-top:1px solid #ccc;
}

.error-message{
	color:#f60;
}
.view-list.dl-horizontal{
	margin:10px 0;
}
.view-list.dl-horizontal dt{
	text-align:left !important;
}
.view-list.dl-horizontal dt,.view-list.dl-horizontal dd{
	height:30px;
	line-height:30px;
}