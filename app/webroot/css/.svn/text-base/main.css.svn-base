/*
 主css文件
*/
h1{font-size:24px;}
h2{font-size:18px;}
h3{font-size:16px;}
h4{font-size:14px;}
h5{font-size:12px;}
ul,ol{padding-left:0;list-style-type:none;}
body,img,h1,h2,h3,h4,h5,h6,hr,p,blockquote,dl,dt,dd,ul,ol,li,pre,form,fieldset,legend,button,input,textarea,th,td{margin:0;padding:0;}
a{
	color:#666;
}
.btn{
	*background:#93abaf;
	*border:1px solid #728e9a;
}
.body{
	color:#333;
	font-size:12px;
}
.nav ul li{
	display:block;
	float:left;
}
.form label{
  	display: inline-block;
	max-width:100px;
}

/*inline form*/
form.inline{
	line-height:40px;
	height:40px;
	vertical-align:middle;
}
form.inline *{margin-bottom:0px;}

/*cake form*/
.b-form .input{
	margin-bottom:10px;
}
.b-form .input label{
	display:inline-block;
	min-width:150px;
}

.none{
	display:none;
}
/*图标*/
.icon{
	display:inline-block;
	background-image: url('../img/icon-8.png');
	background-repeat:no-repeat;
	padding:0 3px;
	width: 14px;
	height: 14px;
	margin-top: 1px;
	*margin-right: .3em;
	line-height: 14px;
	vertical-align: text-top;
}

.icon.icon-user{
	background-position: -41px -11px;
	width: 13px;
	height: 12px;
}
.icon.icon-logout{
	background-position: -76px -11px;
	width: 11px;
	height: 12px;
}

#header{
	height:109px;
	border-bottom:1px solid #aab8bb;
	padding:0px 20px;
	background:url(../img/header_bg.jpg) repeat-x;
	position:relative;
}
#header .top{
	height:35px;
	overflow:hidden;
}
#header .logo h1{
	display:inline-block;
	height:109px;
	background: url('../img/icon-8.png') no-repeat -122px -262px;
	width: 112px;
	height: 18px;
	text-indent:-9999px;
	cursor:pointer;
}
#header .drop-menu{
	border: 1px solid #9999cc;
	position:absolute;
	z-index:99999;
	left:90px;
	top:30px;
	padding:1px;
	background:#fff;
	width:150px;
	line-height:20px;
	padding-bottom:10px;
}
#header .drop-menu li{
	display:inline-block;
	height:20px;
	width:150px;
	line-height:26px;
	text-indent:26px;
}
#header .drop-menu li a{
	color:#666;
}
#header .drop-menu li a:hover{
	font-weight:bold;
}
#header .drop-menu li.title{
	height:30px;
	line-height:30px;
	font-size:14px;
	background:#e4eff3;
}
#header .drop-menu li.dived{
	padding-top:10px;
	height:10px;
}
#header .drop-menu li.dived{
	line-height:30px;
}
#header .nav{
	display:block;
	margin-left:20px;
	width:460px;
	font-size:14px;
}
#header .nav li{
	width:88px;
	height:34px;
	line-height:35px;
	text-align:center;
	color:#333;
}
#header .nav li a{
	color:#333;
}
#header .nav li.active{
	background: url('../img/icon-8.png') no-repeat -103px 0;
}
#header .logo,#header .nav,#header .user-tool{
	height:35px;
	line-height:35px;
}
#header .logo,#header .nav{
	float:left;
}
#header .user-tool{
	float:right;
}
#header .user-tool span{
	padding:0 10px;
}

#header .panel {
}
#header .panel .sub-menu{
	display:block;
	float:left;
}
#header .panel .sub-menu li{
	display: block;
	float:left;
	margin:0 5px;
}
#header .panel .sub-menu li a{
	display: block;
	position:relative;
	width:80px;
	height:71px;
	text-align:center;
}
#header .panel .sub-menu li a span{
	display:inline-block;
	margin-top:50px;
	color:#666;
}
#header .panel .status{
	float:right;
	text-align:right;
	margin-right:20px;
	width: 400px;
}
